{
    "name": "key-values-vscode",
    "displayName": "KeyValues.vscode",
    "description": "Language support for KeyValues (also called Valve Data Format).",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "keyvalues",
            "aliases": ["KeyValues", "keyvalues", "VDF", "Valve Data Format"],
            "extensions": [".txt",".vdf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "keyvalues",
            "scopeName": "source.keyvalues",
            "path": "./syntaxes/keyvalues.tmLanguage.json"
        }]
    }
}